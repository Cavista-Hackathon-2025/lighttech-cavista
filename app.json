{
  "expo": {
    "name": "mediverify",
    "slug": "mediverify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-med.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-med.png",
        "backgroundColor": "#0000"
      },
      "package": "com.light.mediverify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-med.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "64fc3c01-2c38-471a-a695-be4a90246fa7"
      }
    }
  }
}
